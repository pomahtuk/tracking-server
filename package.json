{
  "name": "tracking_server",
  "version": "0.0.0",
  "private": true,
  "description": "Laborant server part",
  "main": "server.js",
  "engines": {
    "node": ">=0.10.32"
  },
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "nodemon server.js",
    "pretest": "npm install",
    "test": "node node_modules/karma/bin/karma start public/test/karma.conf.js",
    "test-single-run": "node node_modules/karma/bin/karma start public/test/karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor public/test/protractor-conf.js"
  },
  "author": "pman",
  "license": "MIT",
  "dependencies": {
    "boom": "^2.5.1",
    "catbox-memory": "^1.1.1",
    "geoip-lite": "^1.1.4",
    "good": "^2.3.0",
    "handlebars": "^2.0.0",
    "hapi": "^7.5.2",
    "jade": "^1.7.0",
    "joi": "^4.8.1",
    "lodash": "^2.4.1",
    "mongoose": "^3.8.17",
    "scooter": "^1.3.0"
  }
}
